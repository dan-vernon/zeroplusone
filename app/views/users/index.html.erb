<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% @users.each do |user| %>
  <ul>
    <li> <%= link_to user.first_name, users_show_path(user) %> </li>
    <li><%= user.location %></li>
    <li><%= ((Date.today - user.dob)/365).floor %> years old</li>
    <li><%= user.bio %></li>
  </ul>
<% end %>
